  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "pnpm run clean && tsc --project tsconfig.json && vite build",
    "preview": "vite preview",
    "prod:remote": "vite build && vite preview --port 3100",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "node scripts/check.test.js",
    "sonar": "pnpm test:coverage && source .env.sonar.local && sonar-scanner -Dsonar.token=$SONAR_TOKEN"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
